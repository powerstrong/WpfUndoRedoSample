<Window
    x:Class="WpfEFTest.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfEFTest"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="300"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <StackPanel Width="200" Margin="5">
            <Button Margin="5" Content="add a joint angle" />
            <Button Margin="5" Content="add a primitive object" />
            <Button Margin="5" Content="show joint angle list" />
            <Button Margin="5" Content="show primitive object list" />
        </StackPanel>

    </Grid>
</Window>
